<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegisterController</servlet-name>
        <servlet-class>coynecontroller.RegisterControllerCustomer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>coynecontroller.LoginControllerCustomer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginControllerAdmin</servlet-name>
        <servlet-class>coynecontroller.LoginControllerAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerTambahProduk</servlet-name>
        <servlet-class>coynecontroller.ControllerTambahProduk</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerEditProduk</servlet-name>
        <servlet-class>coynecontroller.ControllerEditProduk</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerKeranjangCustomer</servlet-name>
        <servlet-class>coynecontroller.ControllerKeranjangCustomer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchHalamanServlet</servlet-name>
        <servlet-class>coynecontroller.SearchHalamanServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterController</servlet-name>
        <url-pattern>/RegisterController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginControllerAdmin</servlet-name>
        <url-pattern>/LoginControllerAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerTambahProduk</servlet-name>
        <url-pattern>/ControllerTambahProduk</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerEditProduk</servlet-name>
        <url-pattern>/ControllerEditProduk</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerKeranjangCustomer</servlet-name>
        <url-pattern>/ControllerKeranjangCustomer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchHalamanServlet</servlet-name>
        <url-pattern>/SearchHalamanServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
